insertPlayer:
INSERT OR IGNORE INTO player_model(id, name, birth_date, height, weight, range, updated_at)
VALUES (?, ?, ?, ?, ?, ?, ?);

selectAll:
SELECT * FROM player_model;

selectPlayerById:
SELECT * FROM player_model WHERE id = ?;

updateInfo:
UPDATE player_model
SET first_name = ?, last_name = ?, is_foreign = ?
WHERE id = ?;