insertPlayer:
INSERT INTO player_model(id, birth_date, height, weight, range)
VALUES (?, ?, ?, ?, ?)
ON DUPLICATE KEY UPDATE
birth_date = VALUES(birth_date),
height = VALUES(height),
weight = VALUES(weight),
range = VALUES(range);

getPlayer:
SELECT * FROM player_model;

getPlayerById:
SELECT * FROM player_model WHERE id = ?;