import java.time.OffsetDateTime;
import java.util.UUID;

CREATE TABLE IF NOT EXISTS user (
  id INTEGER PRIMARY KEY AUTO_INCREMENT,
  subscription_key VARCHAR(255) AS UUID NOT NULL UNIQUE,
  device_id VARCHAR(255) AS UUID NOT NULL UNIQUE,
  date TEXT AS OffsetDateTime NOT NULL
);

insertUser:
INSERT INTO user (subscription_key, device_id, date)
VALUES (?, ?, ?);

selectAllUsers:
SELECT * FROM user;

selectAllUsersBySubscriptionKey:
SELECT * FROM user
WHERE subscription_key = ?;